# CUSTOM WIDGETS
class ConsoleTxt(tk.Text):
	def __init__(self, parent):
		super().__init__(parent)
		self["height"] = 12
		self["width"] = 40
		self["bg"] = "#000000"
		self["fg"] = "#fcfcfc"
		self["insertbackground"] = "#fcfcfc"	# change cursor color of text widget
		self["bd"] = 0
		self["relief"] = "flat"

class CommandEnt(tk.Entry):
	def __init__(self, parent, textvariable):
		super().__init__(parent)
		self["width"] = 30
		self["bg"] = "#000000"
		self["fg"] = "green"
		self["bd"] = 0
		self["relief"] = "flat"
		self["font"] = ("Calibri", 10, "normal")
		self["textvariable"] = textvariable
		self["insertbackground"] = "#fcfcfc"	# change cursor color of text widget

class App(tk.Tk):
	def __init__(self):
		super().__init__()
		self.title("rAt ClIeNt")
		self.iconbitmap(resource_path('rat.ico'))
		self.columnconfigure(0, weight=1)
		self.rowconfigure(0, weight=1)
		self.initvars()
		self.drawgui()
		self.drawprompt(self.console, self.cmdent)
		
	def initvars(self):
		self.cmd = tk.StringVar()
				
	def drawgui(self):
		self.console = ConsoleTxt(self)
		self.console.grid(row=0, column=0, sticky="nsew", padx=4, pady=4)
		self.cmdent = CommandEnt(self.console, textvariable=self.cmd)
						
		self.cmdbut = tk.Button(self, text="Run Command")
		self.cmdbut.grid(row=1, column=0, sticky="ns", padx=4, pady=10)
		
	def drawprompt(self, con, ent, newline=False):
		""" Emulate a CMD prompt using tkinter text passed in as con and ent """
		if newline == False:
			con.insert(tk.END, str(os.getcwd()).strip() + ">")			
		else:
			con.insert(tk.END, "\n" + str(os.getcwd()).strip() + ">")
			
		con.window_create(tk.INSERT, window=ent)
		ent.focus_set()
		ent.bind("<Return>", lambda e: self.runcmd())
			
	def runcmd(self):
		data = str(self.cmd.get()).rstrip()
		if data[:2] == 'cd':
			if os.path.isdir(data[3:]):
				os.chdir(data[3:])
			else:
				self.console.insert(tk.END, "\nError invalid path")
		if data[:2] != 'cd' and len(data[:]) > 0:
			if data[:3] == 'cls':
				self.console.delete('1.0', tk.END)
				self.cmd.set('')
				self.cmdent = CommandEnt(self.console, textvariable=self.cmd)
				self.drawprompt(self.console, self.cmdent)
				self.console.see(tk.END)
				return False # must return here so we exit function and don't get
							 # the second prompt if cls is entered
			else:
				command = Popen(data[:], shell=True, stdout=PIPE, stdin=PIPE, stderr=PIPE)
				output_bytes = command.stdout.read() + command.stderr.read()
				output_str = output_bytes.decode("utf-8")
				self.console.insert(tk.END, "\n" + output_str + "\n")
				self.console.see(tk.END)
				
		self.cmd.set('')
		self.drawprompt(self.console, self.cmdent, newline=True)
		self.console.see(tk.END)		
						
root = App()
root.geometry("500x400+20+20")
root.mainloop()
